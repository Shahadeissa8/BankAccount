@model ViewProfileViewModel
@{
    ViewData["Title"] = "Your profile";
}

<h1 class="text text-center">Your profile</h1>

<table class="table table-bordered text text-center">
    <tr>
    <td>@Html.DisplayNameFor(x=>x.Name)</td>
    <td>@Html.DisplayNameFor(x=>x.Balance)</td>
    </tr>
    <tr>
        <td>
            @Context.Session.GetString("Name")
        </td>
        <td>
            @Context.Session.GetString("Balance")
        </td>
    </tr>
</table>